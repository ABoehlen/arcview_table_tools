(SEd.164
	Name:	"TableTools.AddField"
	Creator:	"adrian"
	CreationDate:	"Fre 24 Jan 2014 17:01:13 CET"
	GUIName:	"Script"
	Win:	165
	CSMgr:	166
	DocExts:	167
	Source:	"'########################################################\n'#\n'# Filename:     tabletools_add_field.ave\n'# Author:       Adrian Boehlen\n'# Date:         24.01.2014\n'# Version:      0.1\n'# Based on:     Table.AddField\n'#\n'# Purpose:      fuegt ein neues Feld der Tabelle hinzu             \n'#\n'########################################################\n\n'# die Tabelle muss im Edit-Modus sein\n\ntheTable = av.GetActiveDoc\ntheVTab = theTable.GetVTab\n\nif (theVTab.CanEdit.Not) then\n  MsgBox.Info(\"Die Tabelle kann nicht
 bearbeitet werden\",\"Abbruch\")\n  return nil\nend\n\nif (theVTab.IsEditable.Not) then\n  theVTab.StartEditingWithRecovery\n  theEditstatus = 0\nelse\n  theEditstatus = 1\nend\n\ntheTable = av.GetActiveDoc\ntheFld = theTable.MakeField\n\nif (theFld <> NIL) then\n  theTable.GetVTab.AddFields({theFld})\n  theTable.SetActiveField(theFld)\nend\n\n'# Editsession beenden, wenn zu Beginn nicht gestartet\n\nif (theEditstatus = 0) then\n  theVTab.SetEditable(false)\nend"
)

